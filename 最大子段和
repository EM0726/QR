#include<iostream>
using namespace std;
int aha[200000];
int dp[200000];
int main()
{
	int n;
	cin>>n;
	for(int i=0;i<n;i++)
		cin>>aha[i];
	dp[0]=aha[0];
	int ans=dp[0];
	for(int i=1;i<n;i++)
	{
		if(dp[i-1]>0)
			dp[i]=dp[i-1]+aha[i];
		else
			dp[i]=aha[i];
		if(dp[i]>ans)
			ans=dp[i];
	}
	cout<<ans<<endl;
	return 0;
}
